{
    "name": "outsanity/ip-analysis",
    "description": "Library for analyzing IP addresses",
    "type": "library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Doug Harple",
            "email": "dharple@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.2",
        "symfony/http-foundation": ">=6.4.29"
    },
    "require-dev": {
        "outsanity/phpcs": ">=0.4.0",
        "phpstan/phpstan": "^2.1.32",
        "phpunit/phpunit": "^9.6.29",
        "rector/rector": "^2.2.8",
        "squizlabs/php_codesniffer": "^3.13.5",
        "symfony/dotenv": "^6.4.24",
        "symfony/flex": "^2.10.0",
        "symfony/serializer": "^6.4.27"
    },
    "suggest": {
        "symfony/serializer": "Serialization support for SpecialAddressBlock"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/flex": true
        },
        "audit": {
            "abandoned": "fail"
        }
    },
    "autoload": {
        "psr-4": {
            "Outsanity\\IpAnalysis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Outsanity\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpcbf":  "phpcbf",
        "phpcs":   "phpcs",
        "phpstan": "phpstan analyse",
        "test":    "phpunit"
    }
}
